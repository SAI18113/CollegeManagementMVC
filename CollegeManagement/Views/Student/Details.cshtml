

@model IEnumerable<CollegeManagement.Models.Student>
@{
    ViewBag.Title = "Student";

}
@{
    var item = (Student)ViewData["student"];
}

<input asp-for="@item.Id" hidden />
<input asp-for="@item.UserId" hidden />
<div>
    <h3 class="text-primary"><b>Student Details</b></h3>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <b>First Name:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.FirstName)
        </dd>
        <dt class="col-sm-2">
            <b>Last Name:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.LastName)
        </dd>
        <dt class="col-sm-2">
            <b>Address:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.Adress)
        </dd>
        <dt class="col-sm-2">
            <b>Mobile:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.Mobile)
        </dd>
        <dt class="col-sm-2">
            <b>Age:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.Age)
        </dd>
        <dt class="col-sm-2">
            <b>State:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.State)
        </dd>
        <dt class="col-sm-2">
            <b>City:</b>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(modelItem => item.City)
        </dd>
    </dl>
</div>



